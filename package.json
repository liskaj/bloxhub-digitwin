        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "fork-ts-checker-webpack-plugin": "^4.0.3",
        "source-map-loader": "^0.2.4",
        "ts-loader": "^6.2.1",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslint": "^6.0.0",
        "typescript": "^3.7.5",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
